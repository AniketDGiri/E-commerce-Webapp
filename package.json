    "exec": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AniketDGiri/Pokemon-Backend.git"
  },
  "author": "Aniket D. Giri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AniketDGiri/Pokemon-Backend/issues"
  },
  "homepage": "https://github.com/AniketDGiri/Pokemon-Backend#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.2",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "winston": "^3.11.0"
  },
  "release": {
    "branches": [
